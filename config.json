{
  "source": ["tokens/**/*.json"],
  "platforms": {
    "scss": {
      "transforms": [
        "attribute/cti",
        "name/cti/kebab",
        "color/hex",
        "myTransitiveTransform"
      ],
      "transformGroup": "scss",
      "buildPath": "src/",
      "files": [
        {
          "destination": "scss/colors.scss",
          "format": "scss/variables",
          "options": {
            "outputReferences": true
          }
        }
      ]
    }
  }
}
